@model Certificates2024.Models.CandidateCertificate


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>CandidateCertificate</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Candidate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Candidate.CandidateId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CertificateTopic)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CertificateTopic.CertificateTopicId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ExaminationDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ExaminationDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CandidateScore)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CandidateScore)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.MaximumScore)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MaximumScore)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ResultLabel)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ResultLabel)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="CandidateCertificateId" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
</body>
</html>
